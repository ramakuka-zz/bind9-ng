// Generated by Chef for <%= node['fqdn'] %>
// Local modifications will be overwritten.
// This is the ACL file to use by bind
<% if node[:bind9][:acl] %>
	<% node[:bind9][:acl].each do |name,ip| %>
  acl <%= name %> {
	  <% ip.each do |address| %>
	  <%= address %>;
	  <% end %>
  };
  <% end %>
 <% end %>